    "section_help_text_tLRf6z": {
      "type": "section-help-text",
      "name": "section-help-text",
      "settings": {
        "title": "Final word",
        "description": "<p class=\"section-help-text__text\">We’re not another glove brand.</p>\n<p class=\"section-help-text__text\">We’re the quiet confidence. The focused design. The elite fit.</p>\n<p class=\"section-help-text__text\">We're what keepers wear when they care about their game — not just the badge.</p>\n<p class=\"section-help-text__text\">Hundred. Built to hold. Built to last. Built for goalkeepers.</p>"
      }
    },
    "section_help_faq_YtKRq4": {
      "type": "section-help-faq",
      "blocks": {
        "row_hptaf7": {
          "type": "row",
          "settings": {
            "faq-title": "What makes Hundred gloves different from other brands?",
            "faq-description": "We build specifically for goalkeepers — no teamwear, no sideline distractions. We use pro-grade materials, elite fit patterns, and pricing that reflects performance, not marketing fluff."
          }
        },
        "row_GekcJQ": {
          "type": "row",
          "settings": {
            "faq-title": "Are your gloves just for professionals?",
            "faq-description": "No. They’re for serious goalkeepers — whether you're in academy setups, Sunday league, or learning your craft in goal at 9 years old. If you care about your game, they’re for you."
          }
        },
        "row_QJjhf8": {
          "type": "row",
          "settings": {
            "faq-title": "Why are your gloves priced lower than big-name brands?",
            "faq-description": "Because we don’t waste money on fluff. No retail markups. No overpriced endorsements. We sell direct and build with better materials — that’s how we offer premium gloves for less."
          }
        },
        "row_qUPFc7": {
          "type": "row",
          "settings": {
            "faq-title": "Do you use the same latex as top-end gloves?",
            "faq-description": "Yes. Our top models use 4mm pro contact latex — the same compound you’ll find in gloves double the price. Our junior gloves use Giga latex for durability and grip where it matters most."
          }
        },
        "row_ABh8yw": {
          "type": "row",
          "settings": {
            "faq-title": "Are your junior gloves just smaller versions?",
            "faq-description": "Absolutely not. We redesign junior gloves from the ground up — with more durable materials, reinforced seams, and youth-specific fits. They're built for how kids dive, fall, and develop."
          }
        }
