                  content.classList.remove("active");

                  tags.forEach(item => {
                    item.classList.remove("active");
                  });

                  tags[i].classList.add("active");
                  wrapper.querySelector(".desktop-tabs__content").innerHTML = `<div>${newContent}</div>`;

