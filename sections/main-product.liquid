            "availability" : "http://schema.org/{% if variant.available %}InStock{% else %}OutOfStock{% endif %}",
            "price" : {{ variant.price | divided_by: 100.00 | json }},
            "priceCurrency" : {{ cart.currency.iso_code | json }},
            "url" : {{ request.origin | append: variant.url | json }}
          }{% unless forloop.last %},{% endunless %}
        {%- endfor -%}
      ]
    }
  </script>
</section>

<div data-popup-image class="bundle__image">
    <button data-prev class="bundle__image-prev">
        <img src="{{ 'arrow-black.svg' | asset_url }}" alt="">
    </button>
    <div id="image">

    </div>
    <button data-next class="bundle__image-next">
        <img src="{{ 'arrow-black.svg' | asset_url }}" alt="">
    </button>
</div>

<style>
  .bundle__image{
      opacity: 0;
      pointer-events: none;
      transition: .5s all;
      position: fixed;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      background-color: rgba(0, 0, 0, .5);
      z-index: 1000000;
      display: flex;
      align-items: center;
      justify-content: center;
  }

  .bundle__image #image{
      max-width: 600px;
      width: 100%;
      object-fit: contain;
      background-color: #f2f2f2;
  }

  #image .product__media,
  #image .product__media img,
  #image .product__modal-opener,
  #image .product-media-container{
    width: 100%;
  }

  #image .loading-overlay__spinner{
        display: none !important;
  }

  .bundle__image-prev,
  .bundle__image-next{
      position: absolute;
      top: 50%;
      transform: translateY(-50%);
      background-color: rgba(255, 255, 255, .5);
      width: 35px;
      height: 35px;
      border-radius: 100%;
      display: flex;
      align-items: center;
      justify-content: center;
      border: none;
      z-index: 5;
  }

  .bundle__image-prev{
      left: 5px;
  }

  .bundle__image-prev img{
      transform: rotate(180deg);
  }

  .bundle__image-next{
      right: 5px;
  }

  .bundle__image.active{
      opacity: 1;
      pointer-events: all;
  }
    
  @media (max-width: 576px) {
      .bundle__image #image{
          width: 100%;
          height: auto;
          max-width: 400px;
          max-height: 75%;
      }
  }

  .product__title{
    width: 100% !important;
    display: flex !important;
    justify-content: space-between;
    align-items: center;
  }
</style>

<script>
  const images = document.querySelectorAll(".product__media-item");
  const popup_image = document.querySelector("[data-popup-image]");
  const image = popup_image.querySelector("#image");
  let index = -1;

  images.forEach((img, i) => {
    img.addEventListener("click", (e) => {
      popup_image.classList.add("active");
      
      index = i;
      image.innerHTML = img.innerHTML;
    });
  });

  document.querySelector("[data-next]").addEventListener("click", (e) => {
    index += 1;

    if (index > images.length - 1) index = 0;
      
    image.innerHTML = images[index].innerHTML;
  });

  document.querySelector("[data-prev]").addEventListener("click", (e) => {
    index -= 1;

    if (index < 0) index = images.length - 1;

    image.innerHTML = images[index].innerHTML;
  });

  popup_image.addEventListener("click", (e) => {
    if (e.target.classList.contains("bundle__image")) {
      popup_image.classList.remove("active");
    }
  });
</script>

