{% schema %}
{
  "name": "Custom box builder",
  "class": "j-box-builder-section",
  "settings": [
    {
      "type": "text",
      "id": "title",
      "label": "Title"
    },
    {
      "type": "collection",
      "id": "collection",
      "label": "Collection"
    },
